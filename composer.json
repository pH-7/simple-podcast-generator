    "podcast",
    "podcast generator",
    "RSS feed",
    "RSS podcast"
  ],
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PierreHenry\\PodcastGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PierreHenry\\PodcastGenerator\\Unit\\Test\\": "tests/unit"
    }
  },
  "authors": [
    {
      "name": "Pierre-Henry Soria",
      "email": "hi@ph7.me"
    }
  ],
  "require": {
    "php": ">=8.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
